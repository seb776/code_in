             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="35" d:DesignWidth="150"
             xmlns:my="clr-namespace:code_in.Views.MainView.Nodes">
