<UserControl x:Class="code_in.Views.NodalView.NodesElems.Nodes.Assets.GenericItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Border CornerRadius="5" Background="#FF151515" Width="auto" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,5,0,5">
            <StackPanel Orientation="Horizontal">
                <Label x:Name="GenericsNames" HorizontalAlignment="Center" Foreground="White" FontWeight="Bold"></Label>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
